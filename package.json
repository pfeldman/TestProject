{
  "name": "testSite",
  "version": "2.0.0",
  "description": "Test Site",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "node clean.js",
    "compile": "node -r dotenv/config --harmony bin/compile",
    "lint": "eslint . ./",
    "lint:fix": "npm run lint -- --fix",
    "dev": "node ./node_modules/concurrently/src/main.js --raw --kill-others \"better-npm-run dev\"",
    "build": "better-npm-run build"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon -r dotenv/config bin/server",
      "env": {
        "DEVELOPMODE": true,
        "BE_HOST": "",
        "BE_PORT": "",
        "URL": "/",
        "RELATIVE_PATH": false,
        "NODE_ENV": "local"
      }
    },
    "build": {
      "command": "npm run clean && npm run compile",
      "env": {
        "BE_HOST": "",
        "URL": "/",
        "BE_HTTPS": true,
        "RELATIVE_PATH": false,
        "NODE_ENV": "test"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pfeldman/TestProject"
  },
  "author": "pablo.feldman@globant.com",
  "dependencies": {
    "better-npm-run": "0.0.5",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "enzyme": "^2.4.1",
    "es5-shim": "^4.5.7",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "fs": "0.0.2",
    "history": "^2.0.0-rc2",
    "material-ui": "^1.0.0-alpha.21",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^3.0.0",
    "redux": "^3.0.0",
    "redux-thunk": "^1.0.0",
    "yargs": "^3.18.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "chai": "^1.9.2",
    "cheerio": "^0.19.0",
    "clean": "^4.0.2",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.0",
    "cssnano": "^3.3.2",
    "enzyme": "^1.5.0",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-flow-vars": "^0.1.3",
    "eslint-plugin-react": "^3.14.0",
    "eslint-plugin-standard": "^1.3.1",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.9.0",
